{
    "name": "phplrt/parser",
    "type": "library",
    "description": "The phplrt Parser package.",
    "license": "MIT",
    "keywords": ["parser", "llk", "ll1", "lr", "slr", "language", "php"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/parser"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phplrt/source": "^3.0",
        "phplrt/exception": "^3.0",
        "phplrt/parser-contracts": "^3.0",
        "phplrt/lexer-contracts": "^3.0",
        "phplrt/ast-contracts": "^3.0",
        "phplrt/grammar-contracts": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Parser\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Parser\\Tests\\": "tests"
        }
    },
    "provide": {
        "phplrt/grammar-contracts-implementation": "^3.0",
        "phplrt/parser-contracts-implementation": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

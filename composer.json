{
    "name": "phplrt/parser",
    "type": "library",
    "description": "The phplrt Parser package.",
    "license": "MIT",
    "keywords": ["parser", "llk", "ll1", "lr", "slr", "language", "php"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/parser"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "phplrt/source": "^2.1.1",
        "symfony/polyfill-php73": "~1.12",
        "phplrt/parser-contracts": "^2.1.1",
        "phplrt/lexer-contracts": "^2.1.1",
        "phplrt/ast-contracts": "^2.1.1"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Parser\\": "src"
        }
    },
    "provide": {
        "phplrt/parser-contracts-implementation": "~2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
